# My method
# Runtime: 32 ms, faster than 61.91% of Python3 online submissions for To Lower Case.
# Memory Usage: 14 MB, less than 6.67% of Python3 online submissions for To Lower Case.
class Solution:
    def toLowerCase(self, str): 
        return str.lower()

# ASCII Method
class Solution:
    def toLowerCase(self, str): 
        return "".join(chr(ord(c) + 32) if 65 <= ord(c) <= 90 else c for c in str)

class Solution:
    def toLowerCase(self, str): 
        return "".join(chr(ord(c) + 32) if "A" <= c <= "Z" else c for c in str)
