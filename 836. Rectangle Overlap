# Runtime: 28 ms, faster than 82.33% of Python3 online submissions for Rectangle Overlap.
# Memory Usage: 13.8 MB, less than 76.40% of Python3 online submissions for Rectangle Overlap.

class Solution:
    def isRectangleOverlap(self, rec1: List[int], rec2: List[int]) -> bool:
        if rec2[2] <= rec1[0] or rec2[0] >= rec1[2] or rec2[1] >= rec1[3] or rec2[3] <= rec1[1]:
            return False
        return True
