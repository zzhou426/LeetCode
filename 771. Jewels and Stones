# Runtime: 24 ms, faster than 95.41% of Python3 online submissions for Jewels and Stones.
# Memory Usage: 13.8 MB, less than 74.72% of Python3 online submissions for Jewels and Stones.

class Solution:
    def numJewelsInStones(self, J: str, S: str) -> int:
        count = 0
        for s in S:
            if s in J:
                count += 1
        return count
